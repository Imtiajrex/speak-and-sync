# Copy this file to .env and fill in your OpenRouter API key
# Your actual .env file is gitignored (see .gitignore)

# Required for enabling real AI parsing (otherwise falls back to local simulation)
VITE_OPENROUTER_KEY=

# Optional: override default model (default: anthropic/claude-3.5-sonnet)
VITE_OPENROUTER_MODEL=anthropic/claude-3.5-sonnet

# Optional: base URL for physical / local device API (no trailing slash)
VITE_DEVICE_API_BASE=http://192.168.0.3:8000
